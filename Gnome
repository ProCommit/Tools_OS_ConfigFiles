gsettings set org.gnome.desktop.wm.preferences mouse-button-modifier '<Control>'
gsettings set org.gnome.mutter center-new-windows true
gsettings set org.gnome.mutter overlay-key ''


gsettings list-keys org.gnome.desktop.wm.keybindings


gsettings set org.gnome.shell.extensions.tiling-assistant.tile-left-half "['<Alt>Left']"
gsettings set org.gnome.shell.extensions.tiling-assistant.tile-right-half "['<Alt>Right']"
gsettings set org.gnome.shell.extensions.tiling-assistant.tile-maximize "['<Alt>Up']"
gsettings set org.gnome.shell.extensions.tiling-assistant.restore-window "['<Alt>Down']"

gsettings set org.gnome.shell.extensions.tiling-assistant.tile-bottom-half "['<Alt>KP_2']"
gsettings set org.gnome.shell.extensions.tiling-assistant.tile-top-half "['<Alt>KP_8']"

gsettings set org.gnome.shell.extensions.tiling-assistant.tile-topleft-quarter "['<Alt>KP_7']"
gsettings set org.gnome.shell.extensions.tiling-assistant.tile-topright-quarter "['<Alt>KP_9']"
gsettings set org.gnome.shell.extensions.tiling-assistant.tile-bottomleft-quarter "['<Alt>KP_1']"
gsettings set org.gnome.shell.extensions.tiling-assistant.tile-bottomright-quarter "['<Alt>KP_3']"
